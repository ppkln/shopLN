<!DOCTYPE html>
<html lang="en">
<link>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <title>ProfileDisplay</title>
</head>
<body>
    <div class="container mt-3 p-3">
        <%- include('../views/header') %>
        <div class="row">
            <div class="col-md-8 mx-auto text-center  ">
                <div class="alert alert-primary">
                    <h1 class="display-5"><% Object.values(data).forEach(function(item){ %></h1>
                    <h3 class="display-5">คุณ : <%= item.Fname %> <%= item.Lname %></h3>
                    <h3 class="display-5">เลขสมาชิก : <%= item.code_id %></h3>
                    <h3 class="display-5">Username : <%= item.e_mail %></h3>
                    <% }) %>
                </div>
            </div>
        </div>
        
    </div>

</body>
</html>